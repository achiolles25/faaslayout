<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="STRUCTURE CHARACTERISTICS"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textAlignment="center"
        android:layout_marginBottom="15dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Doors"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:paddingLeft="35dp"
        android:layout_marginBottom="10dp"/>

    <EditText
        android:id="@+id/door1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="1st Floor"/>
    <EditText
        android:id="@+id/door2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="2nd Floor"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Ceiling"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:paddingLeft="35dp"
        android:layout_marginBottom="10dp"/>

    <EditText
        android:id="@+id/ceiling1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="1st Floor"/>
    <EditText
        android:id="@+id/ceiling2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="2nd Floor"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Windows"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:paddingLeft="35dp"
        android:layout_marginBottom="10dp"/>

    <EditText
        android:id="@+id/window1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="1st Floor"/>
    <EditText
        android:id="@+id/window2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="2nd Floor"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Stairs"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:paddingLeft="35dp"
        android:layout_marginBottom="10dp"/>

    <EditText
        android:id="@+id/stairs1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="1st Floor"/>
    <EditText
        android:id="@+id/stairs2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="2nd Floor"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Partition"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:paddingLeft="35dp"
        android:layout_marginBottom="10dp"/>

    <EditText
        android:id="@+id/partition1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="1st Floor"/>
    <EditText
        android:id="@+id/partition2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="2nd Floor"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Wall Finish"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:paddingLeft="35dp"
        android:layout_marginBottom="10dp"/>

    <EditText
        android:id="@+id/wallfinish1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="1st Floor"/>
    <EditText
        android:id="@+id/wallfinish2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="2nd Floor"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Electrical"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:paddingLeft="35dp"
        android:layout_marginBottom="10dp"/>

    <EditText
        android:id="@+id/electrical1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="1st Floor"/>
    <EditText
        android:id="@+id/electrical2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/editform"
        android:inputType="number"
        android:paddingLeft="5dp"
        android:textSize="25sp"
        android:hint="2nd Floor"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next"
        android:background="@drawable/form"
        android:layout_gravity="end"
        android:id="@+id/btn_submitFinal"
        android:layout_marginBottom="15dp"
        android:layout_marginRight="25dp"/>

</androidx.appcompat.widget.LinearLayoutCompat>